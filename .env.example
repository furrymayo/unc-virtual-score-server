# Flask Virtual Scoreboard - Environment Variables Template
# Copy this file to .env and fill in values

# Flask secret key for session signing (generate a secure random string)
FLASK_SECRET_KEY=

# Web server bind address and port
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
FLASK_DEBUG=1

# Optional TCP/UDP ports for scoreboard data
SCOREBOARD_TCP_PORT=5001
SCOREBOARD_UDP_PORT=5002
